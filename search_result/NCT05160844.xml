  <brief_summary>
    <textblock>
      This is an observertional study aimed at Study the association of SIRT1(rs7069102 ) Gene&#xD;
      polymorphism with premature myocardial infarction in young Egyptian patients&#xD;
    </textblock>
  </brief_summary>
  <overall_status>Recruiting</overall_status>
  <start_date type="Actual">September 1, 2021</start_date>
  <completion_date type="Anticipated">February 28, 2022</completion_date>
  <primary_completion_date type="Anticipated">January 30, 2022</primary_completion_date>
  <study_type>Observational</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <observational_model>Case-Control</observational_model>
    <time_perspective>Cross-Sectional</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>Endothelial nitric oxide synthase level</measure>
    <time_frame>6 months</time_frame>
    <description>detect the serum level of Endothelial nitric oxide synthase by ELIZA in nanogram per millilitre (ng/ml)</description>
  </primary_outcome>
  <primary_outcome>
    <measure>SIRT1 Genotyping</measure>
    <time_frame>6 months</time_frame>
    <description>whether homgenetic or heterogenetic</description>
  </primary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Anticipated">100</enrollment>
  <condition>Premature Myocardial Infarction in Young Egyptian Patients</condition>
  <arm_group>
    <arm_group_label>Diseased</arm_group_label>
    <description>young Egyptian patients with premature myocardial infarction</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Control</arm_group_label>
    <description>healthy young Egyptians</description>
  </arm_group>
  <eligibility>
    <study_pop>
      <textblock>
        young egyptian patients under 40 years of age presented with recent MI admitted to CCU at&#xD;
        sohag university hospital.&#xD;
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:&#xD;
&#xD;
          -  All patient with recent MI admitted to Sohag University Hospital , CCU under 40 years&#xD;
&#xD;
        Exclusion Criteria:&#xD;
&#xD;
          -  patients above 40 year old.&#xD;
&#xD;
          -  Patients with malignancies, major trauma or surgery in the previous six months, and&#xD;
             acute or chronic infectious disease&#xD;
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>40 Years</maximum_age>
    <healthy_volunteers>Accepts Healthy Volunteers</healthy_volunteers>
  </eligibility>
