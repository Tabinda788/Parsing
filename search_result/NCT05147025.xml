  <primary_outcome>
    <measure>Root completion</measure>
    <time_frame>2 years</time_frame>
    <description>Continue root maturation by radiographic assessment according to cvek classification ( increase root length)</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Increase dentine thickness</measure>
    <time_frame>2 years</time_frame>
    <description>Increase dentine thickness as response to new formed pulp like tissues</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Lesion healing</measure>
    <time_frame>2 years</time_frame>
    <description>Resolvation of periapical lesion through new bone formation, intact lamina dura, abscence of periapical radiolucency</description>
  </primary_outcome>
  <number_of_arms>1</number_of_arms>
  <enrollment type="Anticipated">60</enrollment>
