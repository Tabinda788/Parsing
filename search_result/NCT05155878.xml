  <brief_title>Prognostic Factors in Periampullary Tumors and Cysts</brief_title>
  <official_title>Prognostic Factors, Operative Care and Short and Long Term Outcomes for Cysts and Tumors in Pancreas and the Periampullary Region</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Umeå University</agency>
      <agency_class>Other</agency_class>
