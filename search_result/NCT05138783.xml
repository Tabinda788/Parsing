  <brief_title>Clinical Performance of Two Daily Disposable Soft Contact Lenses</brief_title>
  <official_title>Clinical Performance of Two Daily Disposable Soft Contact Lenses</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Alcon Research</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
