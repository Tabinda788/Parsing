  </study_design_info>
  <target_duration>1 Year</target_duration>
  <primary_outcome>
    <measure>ICU mortality</measure>
    <time_frame>Through ICU discharge or date of death, whichever comes first [on average 7 days]</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>In-hospital mortality</measure>
    <time_frame>Through hospital discharge or date of death, whichever comes first [on average 14 days]</time_frame>
  </primary_outcome>
