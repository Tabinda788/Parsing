  <brief_title>Percutaneous Laparoscopy for Ovarian Tissue Cryopreservation.</brief_title>
  <official_title>Percutaneous Laparoscopy for Ovarian Tissue Cryopreservation: a Single-center Experience.</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Regina Elena Cancer Institute</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
